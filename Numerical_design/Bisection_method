def f(x):
    a = pow(x,3)
    c = -x
    d = -11
    return a+c+d
a = 0.0
b= 0.0
result = 0
for i in range(-100 ,500,1):
    if f(i)>=1:
        a = i
    elif f(i)<=(-1):
        b = i
    if f(a)*f(b)<=(-1):
        break
x=0
while round(a,3)!=round(b,3):
    x = (a+b)/2
    if f(x) <0:
        b=x
    elif f(x) >0:
        a=x
    elif f(x)==0:
        result = x
        break
print(a)


